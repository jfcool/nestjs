{
  "name": "@acme/api",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "debug": "pnpm exec nest start --debug --watch",
    "debug:brk": "pnpm exec nest start --debug-brk --watch",
    "build": "nest build",
    "start": "node dist/main.js",
    "db:generate": "drizzle-kit generate --config=drizzle.config.js",
    "db:migrate": "pnpm exec tsx src/database/migrate.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.js",
    "db:studio": "drizzle-kit studio --config=drizzle.config.js"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5",
    "@types/socket.io": "^3.0.2",
    "drizzle-kit": "^0.28.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@agentdb/sdk": "^1.1.19",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.6",
    "@sap-cloud-sdk/connectivity": "^4.1.1",
    "@sap-cloud-sdk/http-client": "^4.1.1",
    "@sap/xsenv": "^6.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "axios": "^1.12.0",
    "bcrypt": "^6.0.0",
    "chokidar": "^4.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.36.4",
    "mammoth": "^1.11.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
